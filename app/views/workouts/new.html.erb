<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Create a New Workout</h1>
  <%= simple_form_for @workout do |f| %>
  <%= f.input :title %>
  <%= f.label :type, 'Type of Workout' %>
  <%= f.select :type, Dictionaries.workout_type, { include_blank: true }, { class: 'form-control select override'} %>
  <br />
  <%= f.submit "Create", :class => 'btn btn-primary' %>
  <br />
<% end %>
<br />
</div>
