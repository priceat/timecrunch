<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1> My Team </h1>
   <%= simple_form_for current_user.teams.last, url: team_path(current_user.teams.last), html: { method: :put } do |f| %>
    <%= f.input :name, :label => 'Team Name' %>
    <%= f.input :age_group, :collection => Team::AGEGROUP, :label => 'Team Age Group', :include_blank => false %>
    <br />
    <%= link_to 'Back', dashboard_coach_path, class: 'btn btn-success' %> 
    <%= f.submit "Update", :class => 'btn btn-primary' %>
    <br />
  <% end %>
</div>